{
  "manifest_version": 2,

  "name": "Dankify",
  "description": "dankifies pictures of people with memes",
  "version": "1.0",
  "content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*", "http://aralisza.github.io/memeswapper/test.html"],
        "js": ["jquery-2.2.2.js", "convert.js", "tracking/build/tracking.js", "tracking/build/data/face-min.js"]
    }
],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
