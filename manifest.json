{
  "manifest_version": 2,

  "name": "Memeswapper",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["jquery-2.2.2.js", "convert.js"]
    }
],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"storage"
  ],
  "options_ui": {
  "page": "options.html",
  "chrome_style": true
  } 
}
